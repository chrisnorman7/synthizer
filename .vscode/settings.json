{
	"files.eol": "\n",
	"files.associations": {
		"chrono": "cpp",
		"*.pyx": "python",
	},
	"[python]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 4
	},
	"[markdown]": {
		"editor.wordWrap": "on",
		"editor.quickSuggestions": false,
		"editor.insertSpaces": true,
	},
	"cmake.generator": "ninja",
	"cSpell.words": [
		"Biquad",
		"Fdn",
		"HRTF",
		"Pausable",
		"SYNTHIZER",
		"SYZ",
		"autopxd",
		"spatialization",
		"spatialized"
	]
}
